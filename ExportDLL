<?xml version="1.0" encoding="UTF-8"?>
<project name="wsiedmanagerDLL" default="createJarAndDLL">
    <import file="./build.xml"/>
    <target name="createJarAndDLL" depends="artifact.iedmanager" description="Create .net Library">
        <exec executable="ikvmc.exe" dir="${artifact.output.iedmanager}" >
            <arg value="-target:library"/>
            <arg value="IEDManager.jar"/>
        </exec>
    </target>
</project>

